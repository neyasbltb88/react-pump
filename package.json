{
  "name": "react-pump",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://neyasbltb88.github.io/react-pump/",
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env REACT_APP_DEPLOY_FOLDER=react-pump react-scripts build && npm run deploy:site",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "deploy:site": "node deploy",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "ftp-deploy": "^2.3.3",
    "gh-pages": "^2.1.1"
  }
}
